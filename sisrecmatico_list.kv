#:kivy 1.0

<MyLabelWithBackground>:
	canvas.before:
		Color:			
			rgb: 1, 0, 0
		Rectangle:
			pos: self.pos
			size: self.size

<MyPopupWithBackground>:
	canvas:
		Rectangle:
			source: 'images/layout/background.jpg'
			pos: self.pos
			size: self.size
			
<Sisrecmatico>:
	buttontest: cicyt
	canvas:
		Rectangle:
			source: 'images/layout/background1.png'
			pos: self.pos
			size: self.size
	BoxLayout:
		orientation: 'horizontal'
		size: (1024,768)
		padding: 20
		spacing: 15
		
		Button:
			id: cti
			text: ''
			font_size: 50
			background_normal: 'images/layout/botonesPrincipales/btnCTI_ON.png'
			background_down: 'images/layout/botonesPrincipales/btnCTI_DOWN.png'
			size_hint: (0.8, 0.4)
			on_release: root.do_cti()
		
		Button:
			id: cicyt
			text: ''
			font_size: 50
			background_normal: 'images/layout/botonesPrincipales/btnCYCIT_ON.png'
			background_down: 'images/layout/botonesPrincipales/btnCYCIT_DOWN.png'
			size_hint: (0.8, 0.4)
			on_release: root.do_cicyt()

